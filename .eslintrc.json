{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "indent": [
            "error",
            4
        ],
        "no-unused-vars": 0,
        "no-console": 0,
        "comma-dangle": ["error", "never"]
    }
}